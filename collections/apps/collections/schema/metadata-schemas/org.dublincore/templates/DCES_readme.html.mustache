    <h1>"{{collectionName}}" Data Package README file</h1>

    <section resource="creativeWork" typeof="http://schema.org/CreativeWork">
        <section class="preamble">

            <!-- Collection Information -->
            <section>
                <span class="title">Package Title</span>
                <span id="collection-title" class="value">{{collectionName}}</span>
            </section>

            <section>
                <span class="title">Package Creation Date</span>
                <span id="collection-creation-date" class="value" property="http://schema.org/dateCreated"
                      content="{{package.date.iso}}">{{package.date.long}}</span>
            </section>

            <section>
                <span class="title">Package Size</span>
                <span id="collection-package-size" class="value"
                      property="http://schema.org/name">{{package.size}} bytes</span>
            </section>

            <section>
                <span class="title">GUID</span>
                <span id="collection-guid" class="value" property="http://schema.org/id">{{package.guid}}</span>
            </section>

            <!-- Metadata Schema Information -->
            <section>
                <span class="title">Metadata Schema</span>
                <table>
                    <tr>
                        <th>Name</th>
                        <td id="metadata-schema-name" property="">{{metadataSchema.name}}</td>
                    </tr>
                    <tr>
                        <th>Version</th>
                        <td id="metadata-schema-version" property="">{{metadataSchema.version}}</td>
                    </tr>
                </table>
            </section>


            <!-- Collections App Software Information -->
            <section class="softwareInfo">
                <span class="title">Software Information</span>
                <section property="http://purl.org/dc/terms/creator" typeof="http://schema.org/softwareApplication"
                         resource="">
                    <table>
                        <tr>
                            <th>Generating Software Application</th>
                            <td id="app-name" property="http://schema.org/name">{{appInfo.name}}</td>
                        </tr>
                        <tr>
                            <th>Software Version</th>
                            <td id="app-version" property="http://schema.org/softwareVersion">{{appInfo.version}}</td>
                        </tr>
                        <tr>
                            <th>URLs</th>
                            <td id="app-urls">
                                <ul>
                                    <li><a href="https://cloudstor.aarnet.edu.au" property="http://schema.org/url">
                                        https://cloudstor.aarnet.edu.au</a></li>
                                    <li><a href="http://www.intersect.org.au/" property="http://schema.org/url">
                                        http://www.intersect.org.au/</a></li>
                                    <li><a href="https://www.westernsydney.edu.au" property="http://schema.org/url">
                                        https://www.westernsydney.edu.au</a></li>
                                    <li><a href="https://www.aarnet.edu.au/" property="http://schema.org/url">
                                        https://www.aarnet.edu.au/</a></li>
                                </ul>
                            </td>
                        </tr>
                    </table>
                </section>
            </section>
        </section>

        <!-- Collection Saved Metadata -->
        <section id="metadata" class="metadata">
            <span class="title">Metadata</span>
            {{#metadataCategories}}

                {{#categoryParties}}
                    <!-- {{displayName}} -->
                    <div class="category" data-category-id="categoryParties" data-category="{{displayName}}">
                        <span class="categoryName">{{displayName}}</span>

                        {{#metadataFields}}
                            {{#pCreatorName}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:creator)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/pCreatorName}}

                            {{#pContributorName}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:contributor)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/pContributorName}}

                            {{#pPublisherName}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:publisher)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/pPublisherName}}
                        {{/metadataFields}}
                    </div>
                {{/categoryParties}}


                {{#categoryDescription}}
                    <!-- {{displayName}} -->
                    <div class="category" data-category-id="categoryDescription" data-category="{{displayName}}">
                        <span class="categoryName">{{displayName}}</span>

                        {{#metadataFields}}
                            {{#deTitle}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:title)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deTitle}}

                            {{#deDateIssued}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:date)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deDateIssued}}

                            {{#deDateCreated}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:date)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deDateCreated}}

                            {{#deDescription}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:description)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deDescription}}

                            {{#deIdentifier}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:identifier)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deIdentifier}}

                            {{#deDOI}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:identifier)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deDOI}}

                            {{#deSubject}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:subject)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deSubject}}

                            {{#deAnzsrcFOR}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:subject)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deAnzsrcFOR}}

                            {{#deAnzsrcSEO}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:subject)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deAnzsrcSEO}}
                        {{/metadataFields}}

                        {{#metadataGroups}}
                            {{#deCoverage}}
                                <div class="group" data-group-id="deCoverage" data-group="{{displayName}}">
                                    <span class="groupName">{{displayName}}</span>

                                    {{#occurrences}}
                                        <div class="groupOccurrence">
                                            {{#metadataFields}}

                                                {{#dcoTemporalCoverage}}
                                                    <span class="fieldName">{{displayName}}</span>
                                                    <span class="metadataTag">(dc:coverage)</span>
                                                    {{#occurrences}}
                                                        <span class="fieldValue">{{value}}</span>
                                                    {{/occurrences}}
                                                {{/dcoTemporalCoverage}}

                                                {{#dcoDateFrom}}
                                                    <span class="fieldName">{{displayName}}</span>
                                                    <span class="metadataTag">(dc:coverage)</span>
                                                    {{#occurrences}}
                                                        <span class="fieldValue">{{value}}</span>
                                                    {{/occurrences}}
                                                {{/dcoDateFrom}}

                                                {{#dcoDateTo}}
                                                    <span class="fieldName">{{displayName}}</span>
                                                    <span class="metadataTag">(dc:coverage)</span>
                                                    {{#occurrences}}
                                                        <span class="fieldValue">{{value}}</span>
                                                    {{/occurrences}}
                                                {{/dcoDateTo}}

                                                {{#dcoSpatialCoverage}}
                                                    <span class="fieldName">{{displayName}}</span>
                                                    <span class="metadataTag">(dc:coverage)</span>
                                                    {{#occurrences}}
                                                        <span class="fieldValue">{{value}}</span>
                                                    {{/occurrences}}
                                                {{/dcoSpatialCoverage}}

                                            {{/metadataFields}}
                                        </div>
                                    {{/occurrences}}
                                </div>
                            {{/deCoverage}}
                        {{/metadataGroups}}

                        {{#metadataFields}}
                            {{#deFormat}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:format)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deFormat}}

                            {{#deObjectType}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:type)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deObjectType}}

                            {{#deLanguage}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:language)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deLanguage}}

                            {{#deRelatedObject}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:relation)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/deRelatedObject}}
                        {{/metadataFields}}

                    </div>
                {{/categoryDescription}}


                {{#categoryRights}}
                    <!-- {{displayName}} -->
                    <div class="category" data-category-id="categoryRights" data-category="{{displayName}}">
                        <span class="categoryName">{{displayName}}</span>

                        {{#metadataFields}}
                            {{#riRightsInfo}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:rights)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/riRightsInfo}}

                            {{#riRightsURI}}
                                <span class="fieldName">{{displayName}}</span>
                                <span class="metadataTag">(dc:rights)</span>
                                {{#occurrences}}
                                    <span class="fieldValue">{{value}}</span>
                                {{/occurrences}}
                            {{/riRightsURI}}
                        {{/metadataFields}}

                    </div>
                {{/categoryRights}}

            {{/metadataCategories}}
        </section>

        <!-- Collection File Tree -->
        <section id="file-tree" class="files">
            <span class="title">Files</span>
            <ul>
                {{#fileList}}
                    {{> readme_file_tree}}
                {{/fileList}}
            </ul>
        </section>
    </section>
</article>
</body>
</html>